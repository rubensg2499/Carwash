<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carwash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".agregar_cliente"></activity>
        <activity android:name=".agregar_servicio" />
        <activity android:name=".agregar_insumo" />
        <activity android:name=".agregar_trabajador" />
        <activity android:name=".agregar_usuario" />
        <activity android:name=".info" />
        <activity android:name=".servicios" />
        <activity android:name=".insumos" />
        <activity android:name=".clientes" />
        <activity android:name=".trabajadores" />
        <activity android:name=".usuarios" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>